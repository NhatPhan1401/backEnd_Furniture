{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*.{ts,json}",
      "use": "@vercel/node",
      "config": {
        "installCommand": "yarn install",
        "buildCommand": "yarn build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ]
}

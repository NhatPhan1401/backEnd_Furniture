{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "yarn build"
      }
    },
    { "src": "dist/index.js", "use": "@vercel/node" }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "dist/index.js" }]
}
